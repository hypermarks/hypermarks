icon(name)
  font-family: name;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;


@font-face
  font-family 'icon-remove'
  src unquote("url(data:application/font-woff;charset=utf-8;base64,) format('woff'), url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype')")
  font-weight normal
  font-style normal

.icon-remove
  icon('icon-remove')
.icon-remove:before
  content: "\f00d"

